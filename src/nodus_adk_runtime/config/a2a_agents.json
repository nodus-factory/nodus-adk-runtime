{
  "agents": [
    {
      "name": "weather_agent",
      "endpoint": "http://agent-pool:8000/weather/a2a",
      "card_url": "http://agent-pool:8000/weather/",
      "enabled": true,
      "timeout": 30,
      "description": "Real-time weather forecasts for Spanish cities",
      "capabilities": ["get_forecast"]
    },
    {
      "name": "currency_agent",
      "endpoint": "http://agent-pool:8000/currency/a2a",
      "card_url": "http://agent-pool:8000/currency/",
      "enabled": true,
      "timeout": 30,
      "description": "Currency conversion with real-time exchange rates",
      "capabilities": ["convert", "convert_multiple"]
    },
    {
      "name": "calculator_agent",
      "endpoint": "http://agent-pool:8000/calculator/a2a",
      "card_url": "http://agent-pool:8000/calculator/",
      "enabled": true,
      "timeout": 10,
      "description": "Mathematical calculations without external APIs",
      "capabilities": ["calculate", "percentage"]
    },
    {
      "name": "email_agent",
      "endpoint": "http://agent-pool:8000/email/a2a",
      "card_url": "http://agent-pool:8000/email/",
      "enabled": false,
      "timeout": 30,
      "description": "Send emails with HITL confirmation (DISABLED: use Google Workspace Gmail tools instead)",
      "capabilities": ["send_email", "check_inbox"]
    },
    {
      "name": "hitl_math_agent",
      "endpoint": "http://agent-pool:8000/hitl_math/a2a",
      "card_url": "http://agent-pool:8000/hitl_math/",
      "enabled": true,
      "timeout": 30,
      "description": "Interactive multiplication with HITL user input",
      "capabilities": ["multiply_with_confirmation", "execute_multiplication"]
    },
    {
      "name": "google_workspace_agent",
      "endpoint": "http://agent-pool:8000/google_workspace/a2a",
      "card_url": "http://agent-pool:8000/google_workspace/",
      "enabled": true,
      "timeout": 45,
      "description": "Specialist in Google Workspace (Gmail, Calendar, Drive, Docs, Sheets). Understands Gmail search syntax and chains operations using OpenMemory.",
      "capabilities": ["search_emails", "read_email", "send_email", "list_calendar", "search_drive", "read_document"]
    }
  ],
  "discovery": {
    "enabled": true,
    "refresh_interval_seconds": 300,
    "retry_on_failure": true
  }
}

